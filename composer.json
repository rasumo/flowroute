{
    "name": "laravel-notification-channels/flowroute",
    "description": ":package_description",
    "homepage": "https://github.com/laravel-notification-channels/flowroute",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Whitfield",
            "email": "marvin.whitfield@gmail.com",
            "homepage": "http://www.marvinwhitfield.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Flowroute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Flowroute\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Flowroute\\FlowrouteServiceProvider"
            ]
        }
    }
}
